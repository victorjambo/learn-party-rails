      <div class="col-md-9">
        <div class="col-md-12" style="margin-bottom: 50px;">
          <div class="row">
            <div id="user-profile">
              <div class="col-md-4">
                <h4>About <%= link_to @user.name || @user.email, @user %></h4>
                <%= image_tag(@user.avatar_url, style: "height:250px;border-radius: 5px;") %>
                <p><%= @user.bio %></p>
              </div>
              <div class="col-md-8">
                <h4>
                    <%= @user.videos.last %>
                </h4>
                <iframe width="100%" height="325" src="http://www.youtube.com/embed/<%= @user.videos.last.url %>" allowfullscreen></iframe>
              </div>
            </div>
          </div>
        </div>
        <h3>Videos Posted by <%= link_to @user.name || @user.email, @user %></h3>
        <hr>
        <div id="video-library">



          <div class="row">

            <% @user.videos.each do |video| %>
            <div class="col-sm-4 col-md-4">
              <%= link_to video do %>
                <img class="video-iframe" src="http://img.youtube.com/vi/<%= video.url %>/0.jpg" allowfullscreen="">
              <% end %>
              <span class="pull-right dashboard-float">
                <%= link_to video, id: "favorite_video", class: "favorite_video" do %>
                  <i class="fa fa-heart likesVideo"></i>
                  <span class="favorites-count incognito-text"> <%= video.get_upvotes.size %></span>
                <% end %>
              </span>
              <section class="video">
                <div class="title">
                  <h4><%= link_to video.title, video %></h4>
                </div>
                <p><%= video.title %></p>
              </section>
            </div>
          <% end %>

          </div>


        </div>
